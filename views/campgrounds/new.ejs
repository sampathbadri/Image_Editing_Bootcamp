<%- include ("../partials/header") %>
    <div class="row">
        <h1 style="text-align: center">Add Image!</h1>
        <div style="width: 30%; margin: 25px auto;">
            <form action="/campgrounds" method="POST">
				<div class="form-group">
                    <input class="form-control" type="text" name="name" placeholder="username">
                </div>
                <div class="form-group">
                    <input class="form-control" type="text" name="description" placeholder="editdetails">
                </div>
				<div class="form-group">
                    <input class="form-control" type="text" name="taskname" placeholder="taskname">
                </div>
				<div class="form-group">
					<input class="form-control" type="text" name="file" placeholder="imageurl">
              
            </div>
                <div class="form-group">
                    <button class="btn btn-lg btn-primary btn-block">Submit!</button>
                </div>
			</form>
            <a href="/campgrounds">Go Back</a>
        </div>
    </div>
<%- include ("../partials/footer") %>